{
  "name": "SimaInput",
  "type": "CORE:Blueprint",
  "extends": [
      "CORE:NamedEntity"
  ],
  "attributes": [
      {
          "type": "CORE:BlueprintAttribute",
          "name": "task",
          "attributeType": "string",
          "default": "WorkflowTask"
      },
      {
          "type": "CORE:BlueprintAttribute",
          "name": "workflow",
          "attributeType": "string",
          "default": "workflow"
      },
      {
          "type": "CORE:BlueprintAttribute",
          "name": "report",
          "attributeType": "sima/marmo/report/ReportItemContainer",
          "optional": true,
          "contained": true
      },
      {
          "type": "CORE:BlueprintAttribute",
          "name": "output",
          "attributeType": "string",
          "default": "res",
          "optional": true,
          "contained": true
      }
  ]
}